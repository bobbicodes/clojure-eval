{
  "name": "lang-clojure",
  "version": "0.1.0-alpha.1",
  "description": "Clojure language support for the CodeMirror code editor",
  "scripts": {
    "test": "cm-runtests",
    "prepare": "cm-buildhelper src/clojure.ts",
    "dev": "vite demo",
    "build": "vite build --outDir './../out' --base './' demo",
    "preview": "vite preview"
  },
  "keywords": [
    "editor",
    "code"
  ],
  "author": {
    "name": "Andrea Amantini",
    "email": "andrea@nextjournal.com",
    "url": "http://nextjournal.com"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "sideEffects": false,
  "license": "MIT",
  "dependencies": {
    "@codemirror/language": "^6.0.0",
    "lezer-clojure": "1.0.0-rc.2"
  },
  "devDependencies": {
    "@codemirror/buildhelper": "^0.1.0",
    "vite": "^3.1.0",
    "codemirror": "6.0.1"
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/nextjournal/lang-clojure.git"
  }
}
